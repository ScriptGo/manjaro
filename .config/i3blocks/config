align=center
separator=false
color=#f3f4f5
separator_block_width=20
markup=pango

[moc]
command=./moc
interval=1
label=🎵 

[time]
command=date '+%P %H:%M'
interval=1
label=<span color="#ffb86c">🕛 </span>

[date]
command=date '+%A %m/%d'
interval=60
label=<span color="#8be9fd">📅 </span>

[volume]
command=./volume
interval=once
signal=10

